<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="account" class="com.taggy.spring.eventlistener.Account" />

	<bean id="homeLoan" class="com.taggy.spring.eventlistener.Loan">
		<property name="loanType" value="Home"></property>
	</bean>



	<bean id="beanPostProcessor" class="com.taggy.spring.beanpostprocessor.BeanPostProcessor" />



</beans>